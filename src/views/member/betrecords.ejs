    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Function to handle tab click
            function handleTabClick() {
                // Remove active class from all tab items
                document.querySelectorAll('.fun-tab-item').forEach(item => {
                    item.classList.remove('activeClassName');
                });

                // Add active class to the clicked tab item
                this.classList.add('activeClassName');

                // Update the displayed content based on the clicked tab (you can implement this logic)
                const tabName = this.querySelector('span').textContent;
                console.log("Selected tab:", tabName);
            }

            // Function to handle category dropdown change
            function handleCategoryChange(event) {
                const selectedCategory = event.target.innerText;
                // Update the displayed content based on the selected category (you can implement this logic)
                console.log("Selected category:", selectedCategory);
            }

            // Function to handle date dropdown change
            function handleDateChange(event) {
                const selectedDate = event.target.innerText;
                // Update the displayed content based on the selected date (you can implement this logic)
                console.log("Selected date:", selectedDate);
            }

            // Add click event listeners to tab items
            document.querySelectorAll('.fun-tab-item').forEach(item => {
                item.addEventListener('click', handleTabClick);
            });

            // Add change event listeners to dropdown selectors
            const categoryDropdown = document.querySelector('.ar-searchbar__selector:nth-child(1)');
            const dateDropdown = document.querySelector('.ar-searchbar__selector:nth-child(2)');
            categoryDropdown.addEventListener('change', handleCategoryChange);
            dateDropdown.addEventListener('change', handleDateChange);
        });

// Get the draggable element
var draggableElement = document.getElementById('customerId');
    
    // Variables to store mouse position
    var offsetX = 0;
    var offsetY = 0;

    // Function to handle touch start event
    function handleTouchStart(e) {
        offsetX = e.touches[0].clientX - draggableElement.getBoundingClientRect().left;
        offsetY = e.touches[0].clientY - draggableElement.getBoundingClientRect().top;
    }

    // Function to handle touch move event
    function handleTouchMove(e) {
        e.preventDefault();
        var x = e.touches[0].clientX - offsetX;
        var y = e.touches[0].clientY - offsetY;
        
        // Update the position of the draggable element
        draggableElement.style.left = x + 'px';
        draggableElement.style.top = y + 'px';
    }

    // Add event listeners for touch events
    draggableElement.addEventListener('touchstart', handleTouchStart, false);
    draggableElement.addEventListener('touchmove', handleTouchMove, false);
    </script>
